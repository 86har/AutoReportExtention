{
  "manifest_version": 3,
  "name": "Auto Page Translate",
  "version": "1.0",
  "permissions": ["storage", "contextMenus", "webNavigation"],
  "host_permissions": ["https://www.nnn.ed.nico/courses/*"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.nnn.ed.nico/courses/*"],
      "js": ["content.js"]
    }
  ]
}
